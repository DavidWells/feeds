{
  "name": "feeds-fetcher",
  "version": "1.0.0",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/test-action",
  "license": "ISC",
  "scripts": {
    "js": "NODE_ENV=development webpack",
    "css": "tailwindcss-cli build -c tailwind.config.js -o pages/css/tailwind.css",
    "serve": "eleventy --serve",
    "build": "NODE_ENV=production npm run css && webpack && eleventy",
    "load": "INPUT_OUTPUTDIRECTORY='contents' INPUT_OPMLFILE='feeds.opml' node index.js",
    "test": "ava"
  },
  "ava": {
    "failWithoutAssertions": false
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "@mozilla/readability": "^0.4.1",
    "@octokit/rest": "^18.12.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/html-minifier": "^4.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/mozilla-readability": "^0.2.1",
    "@types/node": "^16.11.1",
    "@types/puppeteer": "^5.4.4",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "@types/sanitize-html": "^2.5.0",
    "@types/sinon": "^10.0.4",
    "ava": "^3.15.0",
    "date-fns": "^2.25.0",
    "history": "^5.0.1",
    "html-minifier": "^4.0.0",
    "jsdom": "^18.0.0",
    "puppeteer": "^10.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanitize-html": "^2.5.2",
    "sinon": "^11.1.2",
    "tailwindcss-cli": "^0.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "xml2js": "^0.4.23"
  }
}
