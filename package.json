    "react-dom": "^17.0.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@tailwindcss/typography": "^0.4.0",
    "date-fns": "^2.16.1",
    "tailwindcss-cli": "^0.1.2",
    "xml2js": "^0.4.23"
  }
