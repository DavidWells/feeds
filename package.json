{
  "name": "feeds-fetcher",
  "version": "1.0.0",
  "description": "Websites feed fetcher and static feeds aggregator",
  "main": "index.js",
  "author": "Maythee Anegboonlap <null@llun.dev>",
  "repository": "github:llun/test-action",
  "license": "ISC",
  "scripts": {
    "js": "NODE_ENV=development webpack",
    "css": "tailwindcss-cli build -c tailwind.config.js -o pages/css/tailwind.css",
    "serve": "eleventy --serve",
    "build": "NODE_ENV=production npm run css && webpack && eleventy",
    "load": "INPUT_OUTPUTDIRECTORY='contents' INPUT_OPMLFILE='feeds.opml' node index.js",
    "test": "ava"
  },
  "ava": {
    "failWithoutAssertions": false
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@actions/core": "^1.4.0",
    "@actions/github": "^5.0.0",
    "@mozilla/readability": "^0.4.1",
    "@octokit/rest": "^18.6.6",
    "@tailwindcss/typography": "^0.4.1",
    "@types/html-minifier": "^4.0.0",
    "@types/jsdom": "^16.2.12",
    "@types/mozilla-readability": "^0.2.0",
    "@types/node": "^15.14.0",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/sanitize-html": "^2.3.1",
    "@types/sinon": "^10.0.2",
    "ava": "^3.15.0",
    "date-fns": "^2.22.1",
    "history": "^5.0.0",
    "html-minifier": "^4.0.0",
    "jsdom": "^16.6.0",
    "puppeteer": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sanitize-html": "^2.4.0",
    "sinon": "^11.1.1",
    "tailwindcss-cli": "^0.1.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "xml2js": "^0.4.23"
  }
}
