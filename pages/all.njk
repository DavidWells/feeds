---
permalink: "sites/all/"
---
{% extends 'layouts/feeds.njk' %}

{% block categories %}
  <h2><a href="{{github.repository}}/sites/all">All sites</a></h2>
  {% for category in feeds %}
  <h2>
    <a href="{{github.repository}}/categories/{{ category.name | lower }}">{{ category.name }}</a>
  </h2>
  {% endfor %}
{% endblock %}

{% block sites %}
  {% for entry, item in entries %}
    <div>
      <h3>
        <a href="{{github.repository}}/entries/{{ entry | lower }}">{{ item.title }}</a>
      </h3>
    </div>
  {% endfor %}
{% endblock %}
